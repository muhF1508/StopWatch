<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="90" android:endColor="#00251a" android:startColor="#00251a" android:type="linear" />
            <padding android:bottom="0dp" android:left="0dp" android:right="0dp" android:top="0dp" />
            <size android:width="60dp" android:height="60dp" />
            <stroke android:color="#ff3da6ef" />
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="90" android:endColor="#004d40" android:startColor="#004d40" android:type="linear" />
            <padding android:bottom="0dp" android:left="0dp" android:right="0dp" android:top="0dp" />
            <size android:width="60dp" android:height="50dp" />
            <stroke  android:color="#ffcecece" />
            <corners android:radius="50dp"/>

        </shape>
    </item>
</selector>